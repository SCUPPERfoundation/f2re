  <section name='general'>

   <event num='1'>
    <comment>object not found error</comment>
    <message type='single' where='com' who='individual' lo='msg.general.2'/>
   </event>

   <event num='2'>
    <comment>on enter merch stall if have not solved puzz.</comment>
    <checkvariable name='karnakpuzz' key='status' value='complete' equals='stop'/>
    <message type='text' where='com' who='individual'>As you enter the merchant&apos;s stall you over hear him talking with a customer about the troubles at the shrine on Pantheon. It seems the feather of Ma&apos;at is missing from her sacred shrine.</message>
   </event>

   <event num='3'>
    <comment>cannot afford that</comment>
    <message type='text' where='com' who='individual'>You can&apos;t afford that!</message>
   </event>

  </section>

  <section name='sapphire'>

   <event num='1'>
    <comment>pluck or get eye or sapphire</comment>
    <message type='single' where='com' who='individual' lo='msg.sapphire.1'/>
    <freeze />
    <delayevent delay='8' event='event.sapphire.2'/>
   </event>

   <event num='2'>
    <comment>second death</comment>
    <message type='single' where='com' who='individual' lo='msg.sapphire.2'/>
    <delayevent delay='8' event='event.sapphire.3'/>
   </event>

   <event num='3'>
    <comment>third death</comment>
    <message type='single' where='com' who='individual' lo='msg.sapphire.3'/>
    <delayevent delay='8' event='event.sapphire.4'/>
   </event>

   <event num='4'>
    <comment>fourth death</comment>
    <message type='single' where='com' who='individual' lo='msg.sapphire.4'/>
    <delayevent delay='8' event='event.sapphire.5'/>
   </event>

   <event num='5'>
    <comment>check inventory for book</comment>
    <move what='player' who='individual' loc='2014'/>
    <checkinventory map='home' id-name='book' found='event.sapphire.6' not-found='event.sapphire.7'/>
   </event>

   <event num='6'>
    <comment>if has book check var unlocked</comment>
    <checkvariable name='karnakbook' key='locked' value='unlocked' equals='event.sapphire.8' not-equal='event.sapphire.9'/>
   </event>

   <event num='7'>
    <comment>if no book, hospital</comment>
    <message type='single' where='com' who='individual' lo='msg.sapphire.6'/>
    <release />
   </event>

   <event num='8'>
    <comment>unlocked book at hospital</comment>
    <message type='single' where='com' who='individual' lo='msg.sapphire.5'/>
    <message type='single' where='com' who='individual' lo='msg.sapphire.10'/>
    <release />
    <setvariable name='karnakpuzz' key='dead' value='dead'/>
   </event>

   <event num='9'>
    <comment>locked book at hospital</comment>
    <message type='multi' where='com' who='individual' lo='msg.sapphire.9' hi='msg.sapphire.8'/>
    <release />
   </event>

  </section>

  <section name='scarab'>

   <event num='1'>
    <comment>steal take get scarab from merchant</comment>
    <message type='single' where='com' who='individual' lo='msg.scarab.2'/>
    <changestat who='individual' stamina='-5' change='add' cur-max='current'/>
   </event>

   <event num='2'>
    <comment>buy scarab-check money</comment>
    <checkstat who='individual' money='13' higher='event.scarab.9' equals='event.scarab.9' lower='event.general.3'/>
   </event>

   <event num='3'>
    <comment>toss throw drop scarab</comment>
    <message type='single' where='com' who='individual' lo='msg.scarab.1'/>
    <destroyobject where='inventory' map='home' id-name='scarab'/>
   </event>

   <event num='4'>
    <comment>use insert scarab</comment>
    <checkinventory map='home' id-name='book' found='event.scarab.6' not-found='event.scarab.5'/>
   </event>

   <event num='5'>
    <comment>use scarab but no book found</comment>
    <message type='single' where='com' who='individual' lo='msg.general.2'/>
   </event>

   <event num='6'>
    <comment>have book, check if unlocked</comment>
    <checkvariable name='karnakbook' key='locked' value='unlocked' equals='event.scarab.7' not-equal='event.scarab.8'/>
   </event>

   <event num='7'>
    <comment>have book but already unlocked</comment>
    <message type='single' where='com' who='individual' lo='msg.scarab.5'/>
   </event>

   <event num='8'>
    <comment>have locked book so unlock with scarab</comment>
    <message type='single' where='com' who='individual' lo='msg.scarab.4'/>
    <destroyobject where='inventory' map='home' id-name='scarab'/>
    <setvariable name='karnakbook' key='locked' value='unlocked' temporary='true'/>
   </event>

   <event num='9'>
    <comment>buy scarab</comment>
    <changestat who='individual' money='-13' change='add' cur-max='current'/>
    <message type='single' where='com' who='individual' lo='msg.scarab.3'/>
    <createobject where='inventory' home-map='kaos/karnak' id='obj_2scarab'/>
   </event>

  </section>

