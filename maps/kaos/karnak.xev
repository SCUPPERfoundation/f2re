<?xml version="1.0"?>

<event-list version='54'>

 <category name='event'>

  <section name='book'>

   <event num='1'>
    <comment>open book -check locked status</comment>
    <checkvariable name='karnakbook' key='locked' value='unlocked' equals='event.book.20' not-equal='event.book.3'/>
   </event>

   <event num='2'>
    <comment>merchant conversation when enter</comment>
    <checkvariable name='karnakpuzz' key='status' value='complete' equals='stop'/>
    <message type='text' where='com' who='individual'>You hear the merchant discussing with another customer the problems with a shrine on Pantheon.</message>
    <setvariable name='karnakpuzz' key='discussion' value='first' temporary='true'/>
   </event>

   <event num='3'>
    <comment>locked book try to open</comment>
    <message type='single' where='com' who='individual' lo='msg.book.1'/>
   </event>

   <event num='4'>
    <comment>drop book</comment>
    <destroyobject where='inventory' map='home' id-name='book'/>
    <message type='single' where='com' who='individual' lo='msg.book.8'/>
    <checkinventory map='home' id-name='book' found='stop'/>
    <clearvariable name='karnakbook'/>
    <clearvariable name='karnakpage'/>
   </event>

   <event num='5'>
    <comment>book creation event</comment>
    <createobject where='inventory' home-map='kaos/karnak' id='obj_book'/>
    <message type='single' where='com' who='individual' lo='msg.book.9'/>
    <changestat who='individual' money='-42' change='add' cur-max='current'/>
    <setvariable name='karnakbook' key='locked' value='locked'/>
   </event>

   <event num='6'>
    <comment>read book</comment>
    <checkvariable name='karnakbook' key='open' value='open' not-equal='event.book.25'/>
    <checkvariable name='karnakpage' key='bookmark' value='fifth' equals='event.book.7'/>
    <checkvariable name='karnakpage' key='bookmark' value='fourth' equals='event.book.8'/>
    <checkvariable name='karnakpage' key='bookmark' value='third' equals='event.book.9'/>
    <checkvariable name='karnakpage' key='bookmark' value='second' equals='event.book.10'/>
    <checkvariable name='karnakpage' key='bookmark' value='first' equals='event.book.11'/>
   </event>

   <event num='7'>
    <comment>read fifth page</comment>
    <message type='single' where='com' who='individual' lo='msg.book.15'/>
    <setvariable name='karnakbook' key='open' value='closed'/>
    <clearvariable name='karnakpage'/>
   </event>

   <event num='8'>
    <comment>read fourth page</comment>
    <message type='single' where='com' who='individual' lo='msg.book.6'/>
    <setvariable name='karnakpage' key='bookmark' value='fifth'/>
   </event>

   <event num='9'>
    <comment>read third page</comment>
    <message type='single' where='com' who='individual' lo='msg.book.5'/>
    <setvariable name='karnakpage' key='bookmark' value='fourth'/>
   </event>

   <event num='10'>
    <comment>read second page</comment>
    <message type='single' where='com' who='individual' lo='msg.book.4'/>
    <setvariable name='karnakpage' key='bookmark' value='third'/>
   </event>

   <event num='11'>
    <comment>read first page</comment>
    <message type='single' where='com' who='individual' lo='msg.book.3'/>
    <setvariable name='karnakpage' key='bookmark' value='second'/>
   </event>

   <event num='12'>
    <comment>steal book from merchant</comment>
    <checkvariable name='karnaksteal' key='thief' value='yes' equals='event.book.13' not-equal='event.book.14'/>
   </event>

   <event num='13'>
    <comment>has been caught</comment>
    <message type='single' where='com' who='individual' lo='msg.book.12'/>
    <message type='single' where='com' who='individual' lo='msg.book.15'/>
    <changestat who='individual' money='-100' change='add' cur-max='current'/>
    <changestat who='individual' money='-100' change='add' cur-max='current'/>
    <changestat who='individual' money='-100' change='add' cur-max='current'/>
    <changestat who='individual' money='-100' change='add' cur-max='current'/>
    <changestat who='individual' money='-100' change='add' cur-max='current'/>
   </event>

   <event num='14'>
    <comment>has not been caught before</comment>
    <message type='single' where='com' who='individual' lo='msg.book.12'/>
    <message type='single' where='com' who='individual' lo='msg.book.13'/>
    <message type='single' where='com' who='individual' lo='msg.book.14'/>
    <setvariable name='karnaksteal' key='thief' value='yes'/>
    <createobject where='inventory' home-map='kaos/karnak' id='obj_book'/>
    <changestat who='individual' money='-100' change='add' cur-max='current'/>
    <changestat who='individual' money='-100' change='add' cur-max='current'/>
    <changestat who='individual' money='-100' change='add' cur-max='current'/>
    <setvariable name='karnakbook' key='locked' value='locked'/>
   </event>

   <event num='15'>
    <comment>buy book</comment>
    <message type='single' where='com' who='individual' lo='msg.book.16'/>
   </event>

   <event num='16'>
    <comment>sell book</comment>
    <checkvariable name='karnakbook' key='locked' value='locked' equals='event.book.17' not-equal='event.book.18'/>
   </event>

   <event num='17'>
    <comment>book is locked, try to sell</comment>
    <message type='single' where='com' who='individual' lo='msg.book.17'/>
   </event>

   <event num='18'>
    <comment>sell unlocked book-check for location</comment>
    <checkmap map='kaos/karnak'  pass='event.book.23' fail='event.book.24' loc='2087'/>
   </event>

   <event num='19'>
    <comment>clear vars on return book</comment>
    <destroyobject where='inventory' map='home' id-name='book'/>
    <message type='text' where='com' who='individual'>You return the book to the merchant, who takes it from you gratefully.</message>
    <checkinventory map='home' id-name='book' found='stop'/>
    <message type='text' where='com' who='individual'>Your name is cleared from the list of known thieves. And all previous records of your stay on Karnak have been erased.</message>
    <clearvariable name='karnakbook'/>
    <clearvariable name='karnakpuzz'/>
    <clearvariable name='karnakpage'/>
    <clearvariable name='karnaksteal'/>
   </event>

   <event num='20'>
    <comment>open book -check if open already</comment>
    <checkvariable name='karnakbook' key='open' value='open' equals='event.book.22' not-equal='event.book.21'/>
   </event>

   <event num='21'>
    <comment>if closed, then open book</comment>
    <message type='single' where='com' who='individual' lo='msg.book.2'/>
    <setvariable name='karnakbook' key='open' value='open' temporary='true'/>
    <setvariable name='karnakpage' key='bookmark' value='first' temporary='true'/>
   </event>

   <event num='22'>
    <comment>if open cannot open again</comment>
    <message type='single' where='com' who='individual' lo='msg.book.17'/>
   </event>

   <event num='23'>
    <comment>succeed with sell book</comment>
    <message type='single' where='com' who='individual' lo='msg.book.18'/>
    <destroyobject where='inventory' map='home' id-name='book'/>
    <changestat who='individual' money='15' change='add' cur-max='current'/>
    <clearvariable name='karnakbook'/>
    <clearvariable name='karnakpage'/>
   </event>

   <event num='24'>
    <comment>not in right loc to sell book</comment>
    <message type='text' where='com' who='individual'>You can&apos;t find anyone that is willing to buy books here!</message>
   </event>

   <event num='25'>
    <comment>try to read a closed book</comment>
    <message type='text' where='com' who='individual'>You cannot read a closed book!</message>
   </event>

  </section>

