  <section name='tomb'>

   <event num='1'>
    <comment>locked book at hospital</comment>
    <checkvariable name='karnakpuzz' key='dead' value='yes' equals='stop'/>
    <message type='single' where='com' who='individual' lo='msg.tomb.1'/>
    <move what='player' who='individual' loc='2201'/>
   </event>

   <event num='2'>
    <comment>on enter queens tomb</comment>
    <checkinventory map='home' id-name='book' found='stop'/>
    <message type='text' where='com' who='individual'>As you enter the room you realize that you cannot possibly decipher the meanings of the writings on the wall without a reference of some kind to help you translate. Dejected, you turn and leave.</message>
    <move what='player' who='individual' loc='2009'/>
   </event>

   <event num='3'>
    <comment>look at heiroglyph</comment>
    <message type='text' where='com' who='individual'>There are so many depictions of daily life displayed that you pick one at random.</message>
    <call event='msg.tomb.random[2-7]'/>
   </event>

   <event num='4'>
    <comment>on enter common tomb, not dead</comment>
    <checkvariable name='karnakpuzz' key='dead' value='yes' equals='stop'/>
    <move what='player' who='individual' loc='2146'/>
   </event>

   <event num='5'>
    <comment>random get jar event</comment>
    <call event='event.tomb.random[6-9]'/>
   </event>

   <event num='6'>
    <comment>heart for get jar event</comment>
    <createobject where='inventory' home-map='kaos/karnak' id='obj_heart'/>
    <message type='text' where='com' who='individual'>You select one of the jars at random and read the label. This one is labeled: Heart</message>
   </event>

   <event num='7'>
    <comment>guts for get jar event</comment>
    <createobject where='inventory' home-map='kaos/karnak' id='obj_guts'/>
    <message type='text' where='com' who='individual'>You select one of the jars at random and read the label. This one is labeled: Intestines</message>
   </event>

   <event num='8'>
    <comment>on enter common tomb, not dead</comment>
    <checkvariable name='karnakpuzz' key='dead' value='dead' equals='stop'/>
    <move what='player' who='individual' loc='2146'/>
   </event>

   <event num='9'>
    <comment>brain for get jar event</comment>
    <createobject where='inventory' home-map='kaos/karnak' id='obj_brain'/>
    <message type='text' where='com' who='individual'>You select one of the jars at random and read the label. This one is labeled: brain</message>
   </event>

   <event num='10'>
    <comment>drop jar</comment>
    <checkmap map='kaos/karnak'  pass='event.tomb.11' fail='event.tomb.12' loc='2263'/>
   </event>

   <event num='11'>
    <comment>drop jar inside tomb</comment>
    <message type='text' where='com' who='individual'>You gently set the heavy jar back in its place.</message>
   </event>

   <event num='12'>
    <comment>drop jar outside of tomb</comment>
    <message type='text' where='com' who='individual'>As you drop the heavy jar, its contents splatter all over the place making quite a mess!</message>
   </event>

   <event num='13'>
    <comment>get in line</comment>
    <message type='text' where='com' who='individual'>You find a place in line and wait...</message>
    <delayevent delay='8' event='event.tomb.19'/>
    <freeze />
   </event>

   <event num='14'>
    <comment>checkinv for jars</comment>
    <checkinventory map='home' id-name='heart' found='event.tomb.15'/>
    <checkinventory map='home' id-name='liver' found='event.tomb.16'/>
    <checkinventory map='home' id-name='guts' found='event.tomb.17'/>
    <checkinventory map='home' id-name='brain' found='event.tomb.18'/>
   </event>

   <event num='15'>
    <comment>return heart to mummy</comment>
    <destroyobject where='inventory' map='home' id-name='heart'/>
    <message type='text' where='com' who='individual'>You return the jar containing the heart to its proper place.</message>
   </event>

   <event num='16'>
    <comment>return liver to mummy</comment>
    <destroyobject where='inventory' map='home' id-name='liver'/>
    <message type='text' where='com' who='individual'>You return the jar containing the liver to its mummy.</message>
   </event>

   <event num='17'>
    <comment>return guts to mummy</comment>
    <destroyobject where='inventory' map='home' id-name='guts'/>
    <message type='text' where='com' who='individual'>You return the jar containing the intestines to its mummy.</message>
   </event>

   <event num='18'>
    <comment>return brain to mummy</comment>
    <destroyobject where='inventory' map='home' id-name='brain'/>
    <message type='text' where='com' who='individual'>You return the jar containing the brain to its mummy.</message>
   </event>

   <event num='19'>
    <comment>weigh heart after wait in line</comment>
    <message type='text' where='com' who='individual'>After waiting in line for that seems like an unbearably long time, it is finally your turn to step up to the scales. You watch as your heart is weighed against the feather of Ma&apos;at. The scale tips first one way and then the next before settling just barely in your favor.</message>
    <message type='text' where='com' who='individual'>You are then given permission to enter the afterlife with the hope that you will be reincarnated soon as you are obviously deserving of future lives.</message>
    <move what='player' who='individual' loc='1050'/>
    <release />
    <createobject where='inventory' home-map='kaos/karnak' id='feather'/>
    <delayevent delay='5' event='event.tomb.20'/>
   </event>

   <event num='20'>
    <comment>give feather of ma&apos;at to player</comment>
    <message type='text' where='com' who='individual'>You suddenly find that you are holding the feather of Ma&apos;at in your hand!</message>
    <clearvariable name='karnakpuzz'/>
    <setvariable name='karnakpuzz' key='status' value='partial' temporary='true'/>
   </event>

  </section>

  <section name='watch'>

   <event num='1'>
    <comment>Location 2015 check to see if Owner</comment>
    <checkforowner owner='event.watch.2' visitor='event.watch.3'/>
   </event>

   <event num='2'>
    <comment>Yes is owner, create watch</comment>
    <createobject where='inventory' home-map='kaos/karnak' id='obj_watch'/>
    <message type='single' where='com' who='individual' lo='msg.watch.1'/>
   </event>

   <event num='3'>
    <comment>No, is not owner cannot get watch</comment>
    <message type='single' where='com' who='individual' lo='msg.watch.2'/>
    <move what='player' who='individual' loc='2015'/>
   </event>

   <event num='4'>
    <comment>on drop watch</comment>
    <destroyobject where='inventory' map='home' id-name='watch'/>
    <message type='single' where='com' who='individual' lo='msg.watch.3'/>
   </event>

  </section>

 </category>

 <category name='food_event'>

  <section name='lemonade'>

   <event num='1'>
    <comment>to create lemonade to drink free</comment>
