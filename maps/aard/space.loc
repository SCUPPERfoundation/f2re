   <location num='395' flags='sl'>
      <name>Hyperlink</name>
      <desc>  Jump to the galaxy of your choice.</desc>
      <desc>South is the Central Spaceway.</desc>
      <exits s='459'/>
   </location>

   <location num='458' flags='s'>
      <name>Olie</name>
      <desc>   You are orbiting the planet Olie</desc>
   </location>

