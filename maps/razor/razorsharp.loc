      <desc>To the East Is the Insurance Salesmen Office</desc>
      <desc>To the Southwest is Main Street</desc>
      <exits n='389' e='454' se='518' sw='516'/>
   </location>

   <location num='454'>
      <name>RazorSharp&apos;s Insurance Office</name>
