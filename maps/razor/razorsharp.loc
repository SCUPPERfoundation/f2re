      <exits w='453'/>
   </location>

   <location num='463'>
      <name>RazorSharp&apos;s Main Square</name>
      <desc>As your standing in the middle of Razorsharp&apos;s Square you notice a sign:</desc>
      <desc>To the North Is another Bar</desc>
