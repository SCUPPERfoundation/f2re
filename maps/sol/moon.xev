<?xml version="1.0"?>

<event-list version='75'>

 <category name='customscert'>

  <section name='obtain'>

   <event num='1'>
    <comment>player wants to obtain a customs certificate - is staff?</comment>
    <checkflag who='individual' flag ='staff'  set='customscert.obtain.3' unset='customscert.obtain.2'/>
   </event>

   <event num='2'>
    <comment>has a slithy?</comment>
    <checkslithy value='1' pass='customscert.obtain.3' fail='customscert.obtain.4'/>
   </event>

   <event num='3'>
    <comment>OK - fix it all</comment>
    <flipflag who='individual' flag ='customs-cert'/>
    <changeslithy value='-1' change='add'/>
    <message type='text' where='com' who='individual'>You pass over the bribe and are handed your certificate. Don&apos;t forget to renew it when it runs out!</message>
   </event>

   <event num='4'>
    <comment>not a staff member and no slithy</comment>
    <message type='text' where='com' who='individual'>Sadly, you&apos;re not in a position to bribe anyone either for certificates or other useful extras.</message>
   </event>

  </section>

 </category>

