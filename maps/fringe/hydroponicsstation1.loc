   </location>

   <location num='328' flags='e'>
      <name>Exchange</name>
      <desc>A small trading console has been placed here to handle orders and deliveries to the station. Loading and unloading are handled remotely and the entire system is automated.</desc>
      <exits w='327' no-exit='nomove.nomove.1'/>
   </location>

</fed2-map>
