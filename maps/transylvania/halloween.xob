   <object type='static' start='137' visibility='100' size='1' weight='1' flags='i'>
      <name>Pumpkin</name>
      <id>obj_pumpkin</id>
      <desc>A medium-sized pumpkin has been carved into a Jack-O-Lantern with a not so menacing face.  The candle flickers inside the fleshy gourd, casting yellow light all about.</desc>
      <vocab cmd='kiss' event='moves.objects.1'></vocab>
   </object>

   <object type='static' start='136' visibility='100' size='1' weight='1' flags='i'>
      <name>button</name>
      <id>obj_button</id>
