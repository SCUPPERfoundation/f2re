      <vocab cmd='rub' event='party.skull.3'></vocab>
      <vocab cmd='eat' event='party.skull.4'></vocab>
      <vocab cmd='hug' event='party.skull.5'></vocab>
      <vocab cmd='play' event='party.skull.6'></vocab>
   </object>

