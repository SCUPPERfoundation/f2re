<?xml version="1.0"?>

<event-list version='14' editor='Version 1.46'>
   <category name='compass'>
      <section name='check'>

      <event num='1'>
         <comment>Checks if the player has a loaner compass.</comment>
         <checkinventory map='vortex/leucothea' id-name='loaner compass' not-found='compass.check.2'/>
      </event>

      <event num='2'>
         <comment>Checks if the player has a permanent compass</comment>
         <checkinventory map='vortex/leucothea' id-name='PiersXT98 compass' not-found='compass.check.3'/>
      </event>

      <event num='3'>
         <comment>Checks for last location</comment>
         <checklastloc loc='260' pass='compass.check.10' fail='compass.check.4'/>
      </event>

      <event num='4'>
         <comment>Check for last location.</comment>
         <checklastloc loc='198' pass='compass.check.10' fail='move.randomize.1'/>
      </event>

      <event num='10'>
         <comment>High chance of sending a player to a random location.</comment>
         <percent value='75' equals='move.randomize.random[2-20]' lower='move.randomize.random[2-20]'/>
      </event>

      </section>
      <section name='drop'>

      <event num='1'>
         <comment>Destroys dropped loaner compas</comment>
         <destroyobject where='inventory' map='vortex/leucothea' id-name='loaner compass'/>
      </event>

      <event num='2'>
         <comment>Destroys dropped permanent compass</comment>
         <destroyobject where='inventory' map='vortex/leucothea' id-name='PiersXT98 compass'/>
      </event>

      </section>
      <section name='getloaner'>

      <event num='1'>
         <comment>Gives the player a loaner compass</comment>
         <createobject where='inventory' home-map='vortex/leucothea' id='compass'/>
         <message type='text' where='com' who='individual'>The Welcome-Bot gives you a loaner compass.</message>
         <message type='multi' where='com' who='individual' lo='compass.found.260' hi='compass.found.260'/>
      </event>

      </section>
      <section name='getperm'>

      <event num='1'>
         <comment>Checks for PO</comment>
         <checkforowner owner='compass.getperm.4' visitor='compass.getperm.2'/>
      </event>

      <event num='2'>
         <comment>Checks Player&apos;s Funds.</comment>
         <checkmoney amount='750' higher='compass.getperm.3' equals='compass.getperm.3' lower='compass.getperm.5'/>
      </event>

      <event num='3'>
         <comment>Gives the player the compass</comment>
         <changemoney amount='-750'/>
         <message type='text' where='com' who='individual'>Akumai grumbles as he boxes up your new compass, saying, &quot;About danged time you decided to buy something, I ain&apos;t gettin&apos; eeny younger!&quot;</message>
         <message type='text' where='com' who='individual'>Akumai&apos;s expression suddenly changes, and he smiles, &quot;It was a pleasure doing business with you!  Come by again and talk some time!&quot;</message>
         <createobject where='inventory' home-map='vortex/leucothea' id='compass_p'/>
      </event>

      <event num='4'>
         <comment>Gives the PO the compass</comment>
         <message type='text' where='com' who='individual'>Akumai grumbles as he boxes up your new compass, saying, &quot;About danged time you decided to buy something, I ain&apos;t gettin&apos; eeny younger!&quot;</message>
         <message type='text' where='com' who='individual'>Akumai&apos;s expression suddenly changes, and he smiles, &quot;It was a pleasure doing business with you!  Come by again and talk some time!&quot;</message>
         <createobject where='inventory' home-map='vortex/leucothea' id='compass_p'/>
      </event>

      <event num='5'>
         <comment>Informs the player that they don&apos;t have enough money</comment>
         <message type='text' where='com' who='individual'>You don&apos;t have the 750 groats required for that purchase!</message>
      </event>

      <event num='6'>
         <comment>Tells the player to use the buy command instead.</comment>
         <message type='text' where='com' who='individual'>That would be stealing.  Perhaps you want to buy one instead?</message>
      </event>

      </section>
      <section name='loc260'>

      <event num='1'>
         <comment>Checks for a loaner compass</comment>
         <checkinventory map='vortex/leucothea' id-name='loaner compass' found='compass.loc260.4' not-found='compass.loc260.2'/>
      </event>

      <event num='2'>
         <comment>Checks the player&apos;s inventory for a permanent compass.</comment>
         <checkinventory map='vortex/leucothea' id-name='PiersXT98 compass' found='compass.loc260.4' not-found='compass.loc260.3'/>
      </event>

      <event num='3'>
         <comment>Informs the that they should aquire a compass.</comment>
         <message type='multi' where='com' who='individual' lo='compass.notfound.260' hi='compass.notfound.260'/>
         <message type='multi' where='com' who='individual' lo='compass.warn.1' hi='compass.warn.1'/>
      </event>

      <event num='4'>
         <comment>Tells player where the exits are</comment>
         <message type='multi' where='com' who='individual' lo='compass.found.260' hi='compass.found.260'/>
      </event>

      </section>
